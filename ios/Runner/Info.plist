<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleDevelopmentRegion</key>
    <string>$(DEVELOPMENT_LANGUAGE)</string>
    <key>CFBundleDisplayName</key>
    <string>Pain</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>pain</string>
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleShortVersionString</key>
    <string>$(FLUTTER_BUILD_NAME)</string>
    <key>CFBundleSignature</key>
    <string>????</string>
    <key>CFBundleVersion</key>
    <string>$(FLUTTER_BUILD_NUMBER)</string>


    <!-- Add URL schemes for Google Sign-In -->
    <key>CFBundleURLTypes</key>
    <array>
        <dict>
            <key>CFBundleTypeRole</key>
            <string>Editor</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <!-- You need to replace this with your actual reversed client ID from GoogleService-Info.plist -->
                <string>com.googleusercontent.apps.581745483272-dmnlchd65tam2r8lo7drf73d23avnck2</string>
            </array>
        </dict>
    </array>

    <!-- Add Firebase Phone Auth settings -->
    <key>FirebaseAppDelegateProxyEnabled</key>
    <false/>

    <!-- Add reCAPTCHA verification settings -->
    <key>FirebaseScreenReportingEnabled</key>
    <true/>

    <!-- Add permissions for Firebase Phone Auth -->
    <key>NSPhotoLibraryUsageDescription</key>
    <string>This app needs access to your photos to set your profile picture.</string>
    <key>NSCameraUsageDescription</key>
    <string>This app needs access to your camera to take your profile picture.</string>

    <!-- Add these permissions to your Info.plist file (ios/Runner/Info.plist) -->
    <key>NSCameraUsageDescription</key>
    <string>This app needs camera access to take profile pictures</string>
    <key>NSPhotoLibraryUsageDescription</key>
    <string>This app needs photos access to let you select profile pictures from your gallery</string>

    <!-- Add these location permission entries here -->
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>This app needs access to location when open to find people around you.</string>
    <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>This app needs access to location when open and in the background to find people around you.</string>
    <key>NSLocationAlwaysUsageDescription</key>
    <string>This app needs access to location when in the background to find people around you.</string>

    <key>LSRequiresIPhoneOS</key>
    <true/>
    <key>UILaunchStoryboardName</key>
    <string>LaunchScreen</string>
    <key>UIMainStoryboardFile</key>
    <string>Main</string>
    <key>UISupportedInterfaceOrientations</key>
    <array>
       <string>UIInterfaceOrientationPortrait</string>
       <string>UIInterfaceOrientationLandscapeLeft</string>
       <string>UIInterfaceOrientationLandscapeRight</string>
    </array>
    <key>UISupportedInterfaceOrientations~ipad</key>
    <array>
       <string>UIInterfaceOrientationPortrait</string>
       <string>UIInterfaceOrientationPortraitUpsideDown</string>
       <string>UIInterfaceOrientationLandscapeLeft</string>
       <string>UIInterfaceOrientationLandscapeRight</string>
    </array>
    <key>CADisableMinimumFrameDurationOnPhone</key>
    <true/>
    <key>UIApplicationSupportsIndirectInputEvents</key>
    <true/>
</dict>
</plist>